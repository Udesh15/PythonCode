#Sum of Odd,Even elements in List
def SumofList(N):
    Sum =0
    countEven =0 
    countodd =0
    a = []
    N = int(input("Enter the Length of list you want"))
    for i in range(N):
        x = int(input('ENTER THE VALUE YOU WANT IN LIST : '))
        a.append(x)
    for i in a:
        if (i%2==0):
           countEven += 1
        else:
           countodd += 1
    print('No. of Even Value is '+str(countEven),' No of odd value is ' +str(countodd)  )

#def PrintSum():
print(SumofList(5))
