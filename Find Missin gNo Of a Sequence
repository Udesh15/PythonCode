def FindMissingNoOfaSequence(N):
    
    #N = int(input("Enter the length of List"))
    Lst = []
    for i in range(N):
        x = int(input("Enter the value"))
        Lst.append(x)
    MissingElement = Lst[0]   
    print(MissingElement)
    for i in Lst:
        if  i != MissingElement:
            print("The missing Element is : ",str(+MissingElement))
            break
        MissingElement += 1    
    

FindMissingNoOfaSequence(3)
