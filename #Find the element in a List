def SumofList(N):
    Fin_Value = int(input())
    a = []
    Count_Freq = 0
    #N = int(input("Enter the Length of list you want"))
    for i in range(N):
        x = int(input('ENTER THE VALUE YOU WANT IN LIST : '))
        a.append(x)
    print(a)
    for i in a:
        #print(i)
        if(i == Fin_Value):
            Count_Freq += 1
            #print(i)
            #print(a[i])
    print(Count_Freq)
            
        
        
#def PrintSum():
N = int(input('please enter'))
SumofList(N)
